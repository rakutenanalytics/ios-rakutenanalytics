  pod 'SVProgressHUD',              '~> 1.0'
  pod 'RESideMenu',                 '~> 4.0'
  pod 'FontAwesomeKit/FontAwesome', '~> 2.1'
  pod 'HockeySDK',                  '~> 3.8'

end

